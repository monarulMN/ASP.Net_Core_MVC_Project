@model U_OnlineBazer.Models.ProductType
@{
    ViewData["Title"] = "Create";
}
<br />

 <h2 class="text-info">Create Product Type</h2>
 <form asp-action="Create" method="post">
    <div asp-validation-summary="ModelOnly" class="text-danger">

    </div>
    <div class="form-group row">
        <div class="col-2">
            <label asp-for="ProductTypes">ProductType</label>
        </div>
        <div class="col-5">
            <input asp-for="ProductTypes" class="form-control" />
        </div>
        <span asp-validation-for="ProductTypes" class="text-danger"></span>
        <div class="form-group">
            <input type="submit" class="btn btn-primary" value="Save" />
            <a asp-action="Index" class="btn btn-success">Back to List</a>
        </div>
    </div>
</form>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}


